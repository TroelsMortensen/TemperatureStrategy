<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.ThermostatController">
   <children>
      <Button layoutX="442.0" layoutY="243.0" mnemonicParsing="false" onAction="#powerUp" text="Up" />
      <Button layoutX="442.0" layoutY="305.0" mnemonicParsing="false" onAction="#powerDown" text="Down" />
      <Label fx:id="powerLabel" layoutX="444.0" layoutY="281.0" text="power" />
      <TableView fx:id="table" layoutX="-1.0" prefHeight="400.0" prefWidth="375.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="250.0" text="id" />
          <TableColumn fx:id="tempColumn" prefWidth="75.0" text="temp" />
          <TableColumn fx:id="unitColumn" prefWidth="50.0" text="unit" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
